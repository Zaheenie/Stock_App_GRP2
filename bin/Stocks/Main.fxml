<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-border-color: blue;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Stocks.MainController">
