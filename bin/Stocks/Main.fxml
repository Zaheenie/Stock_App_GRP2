      </LineChart>
      <Button layoutX="217.0" layoutY="415.0" mnemonicParsing="false" onAction="#btn" text="Load Graph" />
      <TextField fx:id="userField1" layoutX="29.0" layoutY="36.0" prefHeight="25.0" prefWidth="102.0" promptText="Enter first stock" style="-fx-background-color: green;" />
      <TextField fx:id="userField2" layoutX="169.0" layoutY="36.0" prefHeight="25.0" prefWidth="119.0" promptText="Enter second stock" style="-fx-background-color: green;" />
      <Label fx:id="lbl" layoutX="41.0" layoutY="404.0" prefHeight="55.0" prefWidth="102.0" text="Label" />
